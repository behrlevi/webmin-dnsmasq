config_file=/etc/dnsmasq.conf
dnsmasq_path=/usr/sbin/dnsmasq
pid_filename=/var/run/dnsmasq/dnsmasq.pid
dnsmasq_start=systemctl start dnsmasq
dnsmasq_stop=systemctl stop dnsmasq
dnsmasq_restart=systemctl restart dnsmasq
dnsmasq_reload_files=systemctl reload dnsmasq
dnsmasq_dump_log=/etc/init.d/dnsmasq dump-stats
test_config=1
check_for_updates=1
update_frequency=24
